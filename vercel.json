{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "backend/bot/api/telegram.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/telegram",
      "dest": "/backend/bot/api/telegram.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
