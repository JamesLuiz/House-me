{
  "version": 2,
  "builds": [
    {
      "src": "api/telegram.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/telegram",
      "dest": "api/telegram.py"
    },
    {
      "src": "/api/telegram/(.*)",
      "dest": "api/telegram.py"
    }
  ],
  "functions": {
    "api/telegram.py": {
      "runtime": "python3.9"
    }
  }
}

